import subprocess
import sys


hostip = subprocess.run("host pguat.billdesk.io | awk '{print $4}'" , shell=True, capture_output=True, text=True)

newip = (hostip.stdout)
print(newip)

with open('/home/manojshenoy/OneDrive/python-learning/input.txt') as ip:
    currentip = ip.read()
    print(currentip)

if newip == currentip :
    print("no change")
else: 
    print("ip changed")

